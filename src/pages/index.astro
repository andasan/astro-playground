---
import Layout from '../layouts/Layout.astro';
import Card from '../components/Card.astro';
import { students } from '../data/students';
---

<Layout title="A0522 - Infographic Mini Project">
	{students.map((student) => (
		<Card name={student.name} title={student.title} image={student.image} status={student.status} />
	))}
</Layout>